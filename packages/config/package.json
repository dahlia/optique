{
  "name": "@optique/config",
  "version": "0.10.2",
  "description": "Configuration file support for Optique with Standard Schema validation",
  "keywords": [
    "CLI",
    "command-line",
    "commandline",
    "parser",
    "config",
    "configuration",
    "standard-schema",
    "validation"
  ],
  "license": "MIT",
  "author": {
    "name": "Hong Minhee",
    "email": "hong@minhee.org",
    "url": "https://hongminhee.org/"
  },
  "homepage": "https://optique.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dahlia/optique.git",
    "directory": "packages/config/"
  },
  "bugs": {
    "url": "https://github.com/dahlia/optique/issues"
  },
  "funding": [
    "https://github.com/sponsors/dahlia"
  ],
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.0",
    "deno": ">=2.3.0"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./run": {
      "types": {
        "import": "./dist/run.d.ts",
        "require": "./dist/run.d.cts"
      },
      "import": "./dist/run.js",
      "require": "./dist/run.cjs"
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "@standard-schema/spec": "catalog:"
  },
  "dependencies": {
    "@optique/core": "workspace:*"
  },
  "devDependencies": {
    "@standard-schema/spec": "catalog:",
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "zod": "catalog:"
  },
  "scripts": {
    "build": "tsdown",
    "prepack": "tsdown",
    "prepublish": "tsdown",
    "test": "node --experimental-transform-types --test",
    "test:bun": "bun test",
    "test:deno": "deno test",
    "test-all": "tsdown && node --experimental-transform-types --test && bun test && deno test"
  }
}
